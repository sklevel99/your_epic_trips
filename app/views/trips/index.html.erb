<div class = "container">
   <h1>Your epic trip</h1>
    <div class="list">
       <ul class = "list-unstyled">
          <% @trips.each do |trip| %>
           <li>
             <%= link_to trip.title, trip_path(trip), class: "trip-title" %>
            </li>
           <% end %>
        </ul>
     </div>
     <div>
       <%= link_to "add new trip", new_trip_path, class: "btn btn-sm btn-outline-primary" %>
     <div>
</div>
<div class="footer"></div>
