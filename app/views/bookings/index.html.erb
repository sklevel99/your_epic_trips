<h1>Your bookings</h1>

<ul>
  <% @bookings.each do |booking| %>
    <li><%= booking.start_date%></li>
    <li><%= booking.end_date%></li>
    <%= link_to 'Delete a booking', booking_path(booking), method: :delete, data: { confirm: "Are you sure?" } %>
  <% end %>
</ul>

<%= link_to 'Go back to the lists', trips_path %>
